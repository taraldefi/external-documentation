+    try {
+      const contractAddress = PURCHASE_ORDER_STORAGE_CONTRACT.split(".")[0];
+      const contractName = PURCHASE_ORDER_STORAGE_CONTRACT.split(".")[1];
+
+      const result: any = await fetchReadOnlyFunction({
+        network: network,
+        contractAddress,
